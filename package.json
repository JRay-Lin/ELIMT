{
    "name": "electrical-laboratory-notebook",
    "version": "1.0.0",
    "description": "",
    "main": "server.js",
    "scripts": {
        "install-server": "npm install",
        "install-client": "cd client && npm install",
        "server": "nodemon server.js",
        "local": "nodemon local.js",
        "client": "cd client && npm start",
        "install-dev": "npm install concurrently --save-dev && npm install nodemon --save-dev",
        "install-app": " npm run install-dev && concurrently \"npm run install-server\" \"npm run install-react\"",
        "update-app": "concurrently \"npm run install-server\" \"npm run install-react\"",
        "app": "concurrently \"npm run server\" \"npm run client\"",
        "localapp": "concurrently \"npm run local\" \"npm run client\""
    },
    "author": "CHUN-JUI,LIN",
    "license": "",
    "dependencies": {
        "@google-cloud/local-auth": "^2.1.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "csurf": "^1.11.0",
        "dotenv": "^16.4.4",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "googleapis": "^105.0.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.9",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@flydotio/dockerfile": "^0.5.2",
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.3"
    }
}
